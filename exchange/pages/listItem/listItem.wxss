.container{
    width: 100%;
    height: 100%;
}
.compute-top{
    width: 100%;
    padding-top: 20rpx;
    border-bottom: 2rpx solid #eee;
}
.width90{
    width: 90%;
    margin: auto;
}
.imgBox{
    display: flex;
    justify-content:center;
}
.arrow{
   margin:10rpx 20rpx 0;
}
.compute{
    text-align: center;
    font-size: 34rpx;
    padding: 10rpx 0 ;
}
.currencyImg>image{
    width: 60rpx;
    height: 60rpx;
    border: 1px solid #f2f2f2;
}
.arrow>image{
    width: 50rpx;
    height: 50rpx;
}
.bank-bottom{
    margin:0 20rpx;
    width: 100%;
    margin-bottom: 80rpx;
}
.bankimage{
    width: 50rpx;
    height: 50rpx;
    margin-right: 40rpx;
    margin-top: 17rpx;
}
.bankItem{
    padding: 6rpx 60rpx;
    color: #999;
    font-family: Arial, Helvetica, sans-serif;
    display: flex;
    border-bottom: 1px solid #f2f2f2;
}
.bankItem:nth-last-child(1){
    border-bottom: 0;
}
.bankName{
    font-size: 34rpx;
    height: 85rpx;
    line-height: 85rpx;
}
.bankRate{
    font-size: 30rpx;
    height: 85rpx;
    line-height: 85rpx;
    text-align: right;
    width:100rpx;
    margin-left:50%;
}
.tradeType{
    position: fixed;
    bottom: 0;
    left: 0;
    height: 80rpx;
    width: 100%;
    line-height: 80rpx;
    font-size: 32rpx;
    display: flex;
    background: #fff;
}
.tradeType>view{
    color: #999;
    flex: 1;
    border: 2rpx solid #eee;
    text-align: center;
}
.tradeType>view.active{
    color:black;
}
.bankChoose{
    display: flex;
    width:100%;
    height: 85rpx;
    line-height: 85rpx;
}
.chooseimg{
    margin-left: 54%;
}
.chooseimg>image{
    width: 50rpx;
    height: 50rpx;
    margin: 17rpx;
    display: none;
}
.bankItem .bankActive>image{
    display: block;
}
